{% extends 'base.html.twig' %}

{% block title %}Hello MaterialsController!
{% endblock %}

{% block body %}


	<section class="material_history">
			<table>
				<thead>
					<tr>
						<th>Element</th>
						<th>N° étiquette</th>
						<th>Date d'emprunt</th>
						<th>Date de restitution</th>
						<th>Emprunteur</th>
						<th>Commentaire</th>
					</tr>
				</thead>
				<tbody>
					{% for material in materials %}
						<tr>
							<td>{{ material.name }}</td>
							<td> {{material.tagNumber}} </td>
							<td> {{material.serialNumber}} </td>
							<td>restitution</td>
							<td>Emprunteur</td>
							<td>{{ material.comment }}</td>
						</tr>
		            {% endfor %}
				</tbody>
			</table>

	</section>
{% endblock %}
